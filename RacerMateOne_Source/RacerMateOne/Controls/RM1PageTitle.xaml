<UserControl x:Class="RacerMateOne.Controls.RM1PageTitle"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Visibility="Collapsed"
	HorizontalAlignment="Left"
	VerticalAlignment="Top"
	Name="rm1TitleBar" Loaded="rm1TitleBar_Loaded" Unloaded="rm1TitleBar_Unloaded">
	<Label FontFamily="Arial" FontSize="12" Content="{Binding ElementName=rm1TitleBar,Path=Title}"/>
</UserControl>
